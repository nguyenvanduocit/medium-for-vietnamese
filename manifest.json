{
  "manifest_version": 2,
  "name": "Vietnamese Fonts",
  "description": "Extension helps to fix fonts for Vietnamese.",
  "version": "0.1.4",
  "author": "Được Nguyễn <nguyenvanduocit@gmail.com>",
  "icons": {
    "195": "assets/icons/195.png"
  },
  "background": {
    "scripts": ["assets/background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [
    "assets/css/*"
  ]
}
