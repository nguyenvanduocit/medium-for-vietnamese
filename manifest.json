{
  "manifest_version": 2,
  "name": "Medium For Vietnamese",
  "description": "Extension helps to fix Medium.com's fonts for Vietnamese.",
  "version": "0.1.3",
  "author": "Được Nguyễn <nguyenvanduocit@gmail.com>",
  "icons": {
    "195": "assets/icons/195.png"
  },
  "background": {
    "scripts": ["assets/background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "https://*.medium.com/*"
  ],
  "web_accessible_resources": [
    "assets/*"
  ]
}
